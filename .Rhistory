data <- read_xlsx("C:/Users/StatDep/Desktop/Mobin1.xlsx")
# Reshape the data from wide to long format for each variable
data_long_mda <- data %>%
select(Sample, Group, MDA_Pre_test, MDA_Post_test) %>%
pivot_longer(cols = c("MDA_Pre_test", "MDA_Post_test"),
names_to = "Time",
values_to = "MDA") %>%
mutate(Time = factor(Time, levels = c("MDA_Pre_test", "MDA_Post_test")))
data_long_ldh <- data %>%
select(Sample, Group, LDH_Pre_test, LDH_Post_test) %>%
pivot_longer(cols = c("LDH_Pre_test", "LDH_Post_test"),
names_to = "Time",
values_to = "LDH") %>%
mutate(Time = factor(Time, levels = c("LDH_Pre_test", "LDH_Post_test")))
data_long_ck <- data %>%
select(Sample, Group, CK_Pre_test, CK_Post_test) %>%
pivot_longer(cols = c("CK_Pre_test", "CK_Post_test"),
names_to = "Time",
values_to = "CK") %>%
mutate(Time = factor(Time, levels = c("CK_Pre_test", "CK_Post_test")))
# Combine them for easier analysis
data_long <- data_long_mda %>%
left_join(data_long_ldh, by = c("Sample", "Group", "Time")) %>%
left_join(data_long_ck, by = c("Sample", "Group", "Time"))
# Normality check using Shapiro-Wilk test
mda_shapiro <- data_long %>%
group_by(Time, Group) %>%
shapiro_test(MDA)
print(mda_shapiro)
# Homogeneity of variances check using Levene's test
mda_levene <- data_long %>%
group_by(Time) %>%
levene_test(MDA ~ Group)
print(mda_levene)
res.aov.mda <- anova_test(
data = data_long, dv = MDA, wid = Sample,
between = Group, within = Time
)
get_anova_table(res.aov.mda)
# Post-hoc tests: Paired t-test within each group
pwc.mda <- data_long %>%
group_by(Group) %>%
paired_t_test(MDA ~ Time, p.adjust.method = "bonferroni")
View(data_long)
